<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.common.dao.AddressDao">
    <select id="findAddress" resultType="com.example.common.entity.Address">
        select * from address where account = #{account} limit #{current}, 5;
    </select>
</mapper>